    <style name="DialogTheme" parent="android:Theme.Holo.Light.DialogWhenLarge.NoActionBar">
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <style name="DialogTheme1" parent="@android:style/Theme.Holo.Dialog">

        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@color/light_black</item>
        <item name="android:bottomDark">@color/light_black</item>
        <item name="android:bottomMedium">@color/light_black</item>
        <item name="android:centerBright">@color/light_black</item>
        <item name="android:centerDark">@color/light_black</item>
        <item name="android:centerMedium">@color/light_black</item>
        <item name="android:fullBright">@drawable/progress
        </item>
        <item name="android:fullDark">@drawable/progress</item>
        <item name="android:topBright">@color/light_black</item>
        <item name="android:topDark">@color/light_black</item>
    </style>

    <style name="btn">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="title_bar">
        <item name="android:textColor">#d9f3ff</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="SEGOEUITextViewStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily">SEGOEUI</item>
    </style>

    <style name="SEGOEUIButtonStyle" parent="android:Widget.Holo.Button">
        <item name="android:fontFamily">SEGOEUI</item>
    </style>

    <style name="SEGOEUIEditTextViewStyle" parent="android:Widget.EditText">
        <item name="android:fontFamily">SEGOEUI</item>
    </style>

    <!-- Tab Imageview style Style -->
    <style name="tabImageviewStyle" parent="AppTheme">
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
    </style>

    <!-- Tab Imageview style Style -->
    <style name="tabTxtStyle" parent="AppTheme">
        <item name="android:textColor">@color/tabTextColorNormal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>
