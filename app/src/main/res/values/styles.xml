<resources>

    <!--<style name="generalnotitle" parent="android:Theme.Light">-->
    <style name="generalnotitle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textViewStyle">@style/SEGOEUITextViewStyle</item>
        <item name="android:buttonStyle">@style/SEGOEUIButtonStyle</item>
        <item name="android:editTextStyle">@style/SEGOEUIEditTextViewStyle</item>

        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">#aaaaaa</item>

        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">#80000000</item>

        <!-- colorAccent is used as the default value for colorControlActivated
             which is used to tint widgets -->
        <item name="colorAccent">#20000000</item>

    </style>

    <style name="MyTheme" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:layout_margin">0dp</item>
    </style>

